        local y = root.Position.Y
        local z = root.Position.Z
        local x = root.Position.X + radius*cos(rad(ts))
        v.Position = Vector3.new(x,y,root.Position.Z + radius*sin(rad(ts)))
        ts = ts + 5
    end
end)

