mouse.KeyDown:Connect(function(k)
    if k == "p" then if sitting == false then 
        sitting = true
        sitstartplay:Play() 
        wait(1) 
        sitloopplay:Play()
    elseif sitting == true then 
        sitting = false
        sitloopplay:Stop() 
        sitendplay:Play() 
        end
    end 
end)


