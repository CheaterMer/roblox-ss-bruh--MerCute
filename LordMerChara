--switches
local userInputService = game:GetService("UserInputService")


userInputService.InputBegan:Connect(function(input,gameProcessedEvent)
    local keyPressed = input.KeyCode

      if not gameProcessedEvent then
          if keyPressed == Enum.KeyCode.T then
if game.Players.LocalPlayer.PlayerGui.UI.Ui.Info.Attack.Text == "Star Glitcher" then
            if ModeBind == MaxModes then
                ModeBind = 1
            else

            ModeBind = ModeBind + 1
        end
            if ModeBind == 1 then
                TextLabel.Text = "Mayehm"
            elseif ModeBind == 2 then
                TextLabel.Text = "Divinity"
            elseif ModeBind == 3 then
                TextLabel.Text = "Catastrophe"
            elseif ModeBind == 4 then
                TextLabel.Text = "Purity"
            elseif ModeBind == 5 then
                TextLabel.Text = "Corruption"
            elseif ModeBind == 6 then
                TextLabel.Text = "Fallen-X"
            elseif ModeBind == 7 then
                TextLabel.Text = "VIOLENCE"
            elseif ModeBind == 8 then
                TextLabel.Text = "StarGlitch"
            elseif ModeBind == 9 then
                TextLabel.Text = "createClaws"
            end

          end
  end
  end
  end)









