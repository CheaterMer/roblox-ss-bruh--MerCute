    v.Position = Character.HumanoidRootPart.Position - Vector3.new(0,9,10)
elseif times == 4 then
    v.Position = Character.HumanoidRootPart.Position - Vector3.new(0,9,-10)
elseif times == 5 then
    v.Position = Character.HumanoidRootPart.Position - Vector3.new(-10,9,0)
    end
