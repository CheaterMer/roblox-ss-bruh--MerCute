 local mouse = game.Players.LocalPlayer:GetMouse() --ez
mouse.KeyDown:Connect(function(k) 
    if k == "h" then
	 if game.Players.LocalPlayer.PlayerGui.UI.Ui.Info.Attack.Text == "Music Box" then
	game.Players:Chat(("Song IDK <Sound Max>"))
        --Music
		local pos = char.HumanoidRootPart.CFrame
		local dummy = getDummy()
		if not dummy then
			dummy = rep.Resources.MoveEffects.HateBall.HateBall
		end
		if dummy and dummy:FindFirstChild("HumanoidRootPart") then
			char:SetPrimaryPartCFrame(dummy.HumanoidRootPart.CFrame)
			taskwait(0.2)
			char:SetPrimaryPartCFrame(dummy.HumanoidRootPart.CFrame)
			taskspawn(function()
				remotes.Damage:InvokeServer(pass, dummy, {CombatInv=true,BlockAble=false,Type="Normal",Sound = {rep.Sounds.GTLevelUp,rep.Sounds.HyperGonerLaugh},Damage = 0,HitTime = 0,HurtAnimation = rep.Animations.HurtAnimations["Hurt1"],VictimCFrame = dummy.HumanoidRootPart.CFrame,Velocity=dummy.HumanoidRootPart.CFrame.LookVector,HitEffect="BurstEffect"})
			end)
			taskwait(0.09)
			remotes.Damage:InvokeServer(pass, dummy, {CombatInv=true,BlockAble=false,Type="Normal",Sound = {game:GetService("StarterPlayer").StarterPlayerScripts.Music.Music["5Song"]},Damage = 0,HitTime = 0,HurtAnimation = rep.Animations.HurtAnimations["Hurt1"],VictimCFrame = dummy.HumanoidRootPart.CFrame,Velocity=dummy.HumanoidRootPart.CFrame.LookVector,HitEffect="BurstEffect"})
			taskwait(0.1)
			char:SetPrimaryPartCFrame(pos)
		end
end
    end
	end
)

