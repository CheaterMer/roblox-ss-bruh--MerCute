        taskspawn(function()
            pcall(function()
                remotes.CharaMoves:InvokeServer({pass, "KnifeHit", v, v.CFrame, workspace.DodgingDummy });
            end)
        end)
    end
end
end
end)
elseif Reflection == true then
Reflection = false
