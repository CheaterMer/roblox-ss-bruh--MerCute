local function LockCalculate(lock)
	if not lock.Value then
		return CFrame.new(mouse.Hit.p, mouse.Hit.p + rootpart.CFrame.lookVector * 5);
	end;
	return CFrame.new(lock.Value.HumanoidRootPart.Position + Spot(lock.Value, lock.Value.HumanoidRootPart.CFrame, 5).lookVector * 5, lock.Value.HumanoidRootPart.Position);
end

local function CreateInstance(cls,props)
local inst = Instance.new(cls)
for i,v in pairs(props) do
    inst[i] = v
end
return inst
end

