local root = plr.Character.HumanoidRootPart
local target = lock.Value
if not target then return end
if (target.HumanoidRootPart.Position - rootpart.Position).magnitude > 10000 then return end



local vec =  target.HumanoidRootPart.Position -rootpart.Position
local direction = (vec).Unit
local pos = rootpart.Position + direction*((vec).magnitude+5)
local face= pos+direction*2
rootpart.CFrame = CFrame.new(pos,face)
char.Humanoid:ChangeState(Enum.HumanoidStateType.Flying)
tar = rootpart.CFrame*CFrame.new(0,5,0)
char.Humanoid:ChangeState(Enum.HumanoidStateType.Flying)
v2 = Instance.new('BodyPosition',rootpart) 
fol = Instance.new('Folder',v2)
v2.Name = 'Client' 
v2.MaxForce = Vector3.new(1,1,1)*100000 
v2.D = 1000 
v2.P = 60000 
v2.Position = (tar).p
local v3 = Instance.new('BodyGyro',rootpart)
v3.Name = 'Client'
v3.MaxTorque = Vector3.new(1,1,1)*math.huge
v3.D = 100 ;v3.P = 30000 
v3.CFrame = rootpart.CFrame
wait(0.3)

local on = false

        if on == false then
            local plr = game:service("Players").LocalPlayer.Backpack.Main.LockOnScript.LockOn.Value
            local lockedplr = workspace[plr.Name]
            on = true

            repeat

    local pos = target.HumanoidRootPart.Position+Vector3.new(math.random(-25,25),math.random(0,0),math.random(-25,25))
    v2.Position = pos
    rootpart.CFrame = CFrame.new(pos,target.HumanoidRootPart.Position);v3.CFrame = CFrame.new(pos,target.HumanoidRootPart.Position)





spawn(function()
	local A_1 =  {
      [1] = pass,
      [2] = "KnifeProjectile", 
