Event:InvokeServer(A_1)
end)

                if lockedplr.Humanoid.Health <= 0 then
                    print("Done!")
                    on = false
					v2:Destroy()
					v3:Destroy()
                end
            until lockedplr.Humanoid.Health <= 0
        end
    end
end
end)

