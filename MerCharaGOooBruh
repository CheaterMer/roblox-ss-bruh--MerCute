Credits.TextWrapped = true

-- Scripts:

local function CRQDKY_fake_script() -- Credits.Script 
	local script = Instance.new('Script', Credits)

	local credits = script.Parent
	local text
	
	function SoundEffect ()
		local Sound = Instance.new("Sound",Workspace)
		Sound.Name = "TextSound"
		Sound.SoundId = "http://www.roblox.com/asset/?id=3333976425"
		Sound.PlaybackSpeed = 1
		Sound.Volume = 1
		Sound:Play()
		coroutine.resume(coroutine.create(function()
			wait(1)
			Sound:Destroy()
		end))
	end
	
	function setText(word)
		text = word
		for i = 1, #text do
			credits.Text = string.sub(text, 1, i)
			SoundEffect()
			wait(0.04)
		end
	end
	
	
	setText("Owner: Cheater_Mer#0489")
	wait(1)
	setText("Scripters/Cheater_Mer#0489")
	wait(1)
	setText("This is My Script")
	wait(3)
	setText("Enjoy!")
end
coroutine.wrap(CRQDKY_fake_script)()
wait(12)
MainFrame:Destroy()
Credits:Destroy()







