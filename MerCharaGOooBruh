-- Scripts:

local function TUIIQZV_fake_script() -- Closecmdlist.Close 
	local script = Instance.new('LocalScript', Closecmdlist)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Visible = false
	end)
end
coroutine.wrap(TUIIQZV_fake_script)()
local function ICXZRVI_fake_script() -- Opencmdlist.Open 
	local script = Instance.new('LocalScript', Opencmdlist)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Parent.Overwrite.Visible = true
	end)
end
coroutine.wrap(ICXZRVI_fake_script)()
end
end)











