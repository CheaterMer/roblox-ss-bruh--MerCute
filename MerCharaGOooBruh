CharaSkill.Position = UDim2.new(0.145985395, 0, 0.380104517, 0)
CharaSkill.Size = UDim2.new(0, 97, 0, 33)
CharaSkill.Font = Enum.Font.Roboto
CharaSkill.Text = "CharaSkill"
CharaSkill.TextColor3 = Color3.fromRGB(255, 255, 255)
CharaSkill.TextScaled = true
CharaSkill.TextSize = 14.000
CharaSkill.TextWrapped = true
CharaSkill.MouseButton1Down:connect(function()


		local Event = game:GetService("ReplicatedStorage").Remotes.CharaMoves
		Event:InvokeServer(A_1)
		local A_1 = {
            [1] = getrenv()._G.Pass,
            [2] = "Chatted",
            [3] = "Use Chara Style",
            [4] = Color3.new(1, 0, 0)
        }
        local Event = game:GetService("ReplicatedStorage").Remotes.Events
        Event:FireServer(A_1)




