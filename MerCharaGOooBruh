ReMove.Name = "Skillremove"
ReMove.Parent = ButtonsHolder
ReMove.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
ReMove.BorderColor3 = Color3.fromRGB(255, 255, 255)
ReMove.Position = UDim2.new(0.145985395, 0, 0.600104517, 0)
ReMove.Size = UDim2.new(0, 97, 0, 33)
ReMove.Font = Enum.Font.Roboto
ReMove.Text = "Skillremove"
ReMove.TextColor3 = Color3.fromRGB(255, 255, 255)
ReMove.TextScaled = true
ReMove.TextSize = 14.000
ReMove.TextWrapped = true
ReMove.MouseButton1Down:connect(function()

		local Event = game:GetService("ReplicatedStorage").Remotes.CharaMoves
		Event:InvokeServer(A_1)
		local A_1 = {
            [1] = getrenv()._G.Pass,
            [2] = "Chatted",
            [3] = "ReMove Skill Style",
            [4] = Color3.new(1, 0, 0)
        }
        local Event = game:GetService("ReplicatedStorage").Remotes.Events
        Event:FireServer(A_1)

game.Players.LocalPlayer:GetMouse():Destroy()

end)



