.Dialog="The end Time!"
function PlayAnimation(e)
local f=Instance.new("Animation")f.AnimationId=e;
local g=c.Humanoid;return g:LoadAnimation(f)end;function KillAura(c,h,i,j,k,l)warn('Ran HitBox')for m,n in pairs(game.Workspace:GetChildren())do 
local o=n:IsA("Model")and n or nil;if o and o:FindFirstChild("Humanoid")and o:FindFirstChild("HumanoidRootPart")and o~=c then if(c.HumanoidRootPart.Position-o.HumanoidRootPart.Position).magnitude<=h then 
local p=getrenv()._G.Pass;
local q=game.Workspace[o.Name]
local r={["HitTime"]=j,["Type"]=l,["HitEffect"]="HeavyHitEffect",["VictimCFrame"]=c.Torso.CFrame,["HurtAnimation"]=game:GetService("ReplicatedStorage").Animations.HurtAnimations.Hurt3,["Velocity"]=c.HumanoidRootPart.CFrame.lookVector*k+Vector3.new(1,k,1),["Sound"]=game:GetService("ReplicatedStorage").Sounds.SpellCard,["BlockAble"]=false,["CameraShake"]="BigExplosion",["CombatInv"]=true,["Damage"]=i}pcall(function()spawn(function()game:GetService("ReplicatedStorage").Remotes.Damage:InvokeServer(p,q,r)end)end)end end end end;
local s=true;function ForceChat(t)
local u={[1]={[1]=getrenv()._G.Pass,[2]="Chatted",[3]=t,[4]=Color3.new(1,1,1)}}game:GetService("ReplicatedStorage").Remotes.Events:FireServer(unpack(u))end;d.InputBegan:Connect(function(v)if s==false then return end;if v.KeyCode==Enum.KeyCode.K then if d:GetFocusedTextBox()==nil then spawn(function()s=false;wait(1)s=true end)ForceChat(getgenv().Dialog)PlayAnimation("rbxassetid://7515642035"):Play()PlayAnimation("rbxassetid://7515642035"):AdjustSpeed(1)wait(0.1)KillAura(c,7,getgenv().Damage,1,99,"Knockback")end end end)
local w=1;d.InputBegan:Connect(function(v)if s==false then return end;if v.KeyCode==Enum.KeyCode.N then if d:GetFocusedTextBox()==nil then if w==1 then w=2;getgenv().Damage=35;getgenv().Dialog="bye bye"warn(w)elseif w==2 then w=1;getgenv().Damage="nan"getgenv().Dialog="The True power!"warn(w)end end end end)





-- 콤보 인내



local pass = getrenv()._G.Pass
local player = game.Players.LocalPlayer
local character = player.Character
local uis = game:GetService("UserInputService")
local cooldown = true
game:GetService("UserInputService").InputBegan:Connect(function(input, IsTyping)
    if IsTyping then return end
    if input.KeyCode == Enum.KeyCode.B then
	
	local A_1 = 
		{
			[1] = getrenv()._G.Pass, 
			[2] = "PatienceAttack", 
			[3] = game:GetService("Players").LocalPlayer.Backpack.Main.LockOnScript.LockOn.Value.HumanoidRootPart.Position, 
			[4] = game:GetService("Players").LocalPlayer.Backpack.Main.LockOnScript.LockOn.Value
		}
	local Event = game:GetService("ReplicatedStorage").Remotes.CharaMoves
	Event:InvokeServer(A_1)
	wait(0.25)

	local function punchyy(id)

		local target = player.Backpack:WaitForChild("Main").LockOnScript.LockOn.Value
		if target == nil then return end

		local A_1 = pass
		local A_2 = target
		local A_3 = {
			["HitTime"] = 1,
			["Type"] = "Normal", 
			["HitEffect"] = "KnifeHitEffect",
			["HurtAnimation"] = game:GetService("ReplicatedStorage").Animations.HurtAnimations.Hurt3,  
			["Sound"] = game:GetService("ReplicatedStorage").Sounds.KnifeHit,
			["Velocity"] = Vector3.new(0, -0.05, 0),
			["Damage"] = 10
		}
		local Event = game:GetService("ReplicatedStorage").Remotes.Damage
		Event:InvokeServer(A_1, A_2, A_3)

		local A_1 = 
			{
				[1] = getrenv()._G.Pass, 
				[2] = "SlashEffect", 
				[3] = game:GetService("Players").LocalPlayer.Backpack.Main.CharaMoves.ModuleScript.Animations.Slash.Swing3, 
				[4] = maincolor, 
				[5] = CFrame.Angles(0,0,0)
			}
		local Event = game:GetService("ReplicatedStorage").Remotes.Events
		Event:FireServer(A_1)

		local anim = Instance.new("Animation")
		anim.AnimationId = "rbxassetid://"..id
		local anima = character.Humanoid:LoadAnimation(anim)
		anima:Play()
		anima:AdjustSpeed(2)

	end

	punchyy(4333546395)
	wait(0.02)
	punchyy(4348265296)
	wait(0.02)
	punchyy(4348287123)
	wait(0.02)
	punchyy(4333546395)
	wait(0.02)
	punchyy(4348265296)
	wait(0.02)
	punchyy(4348287123)
	wait(0.02)
	punchyy(4333546395)
	wait(0.02)
	punchyy(4348265296)
	wait(0.02)
	punchyy(4348287123)
	wait(0.02)
	punchyy(4333546395)
	wait(0.02)
	punchyy(4348265296)
	wait(0.02)
	punchyy(4348287123)
	wait(0.02)
	local A_1 = 
		{
			[1] = getrenv()._G.Pass, 
			[2] = "PatienceAttack2", 
			[3] = game:GetService("Players").LocalPlayer.Backpack.Main.LockOnScript.LockOn.Value.HumanoidRootPart.Position, 
			[4] = game:GetService("Players").LocalPlayer.Backpack.Main.LockOnScript.LockOn.Value
		}
	local Event = game:GetService("ReplicatedStorage").Remotes.CharaMoves
	Event:InvokeServer(A_1)
	wait(0.02)
	local target = player.Backpack:WaitForChild("Main").LockOnScript.LockOn.Value
	local A_1 = pass
	local A_2 = target
	local A_3 = {
		["HitTime"] = 1,
		["Type"] = "Knockback", 
		["HitEffect"] = "KnifeHitEffect",
		["HurtAnimation"] = game:GetService("ReplicatedStorage").Animations.HurtAnimations.Hurt3,  
		["Sound"] = game:GetService("ReplicatedStorage").Sounds.KnifeHit,
		["Velocity"] = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.lookVector * 170 + Vector3.new(0, -20, 0),
		["Damage"] = 20
	}
	local Event = game:GetService("ReplicatedStorage").Remotes.Damage
	Event:InvokeServer(A_1, A_2, A_3)

	local A_1 = {
		[1] = getrenv()._G.Pass, 
		[2] = "SlashEffect", 
		[3] = game:GetService("Players").LocalPlayer.Backpack.Main.CharaMoves.ModuleScript.Animations.Slash.Swing3, 
		[4] = maincolor, 
		[5] = CFrame.Angles(0,0,math.rad(90))
	}
	local Event = game:GetService("ReplicatedStorage").Remotes.Events
	Event:FireServer(A_1)

	local anim = Instance.new("Animation")
	anim.AnimationId = "rbxassetid://4906108508"
	local anima = character.Humanoid:LoadAnimation(anim)
	anima:Play()
	anima:AdjustSpeed(1.5)

end
end)
