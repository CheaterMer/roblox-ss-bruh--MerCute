Opencmdlist.BorderColor3 = Color3.fromRGB(255, 255, 255)
Opencmdlist.Position = UDim2.new(-0.0255437493, 0, -0.028069973, 0)
Opencmdlist.Size = UDim2.new(0, 110, 0, 43)
Opencmdlist.Font = Enum.Font.Roboto
Opencmdlist.Text = "Open cmd list"
